<html>
<head>
	<script type="text/javascript" src="http://use.typekit.com/wbv3uxc.js"></script>
	<script type="text/javascript">try{Typekit.load();}catch(e){}</script>
 	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title><%= community_name %></title>
	<meta name="description" content="">
	<meta name="author" content="Jamil Abreu">
	<meta name="viewport" content="width=device-width">
  <%= stylesheet_link_tag    "application", :media => "all" %>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.js"></script>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="wrap">
	<div class="container">
		<%= render "layouts/header" %>
		<div class="row">
			<%= yield %>
		</div>
	</div>
</div>
<%= render "layouts/login" unless user_signed_in? %>
<%= render "layouts/new_message" if user_signed_in? %>
<%= render "layouts/analytics" if Rails.env.production? %>
<%= debug(params) if Rails.env.development? %>
</body>
</html>
